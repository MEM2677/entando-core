<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p" 
	xmlns:jee="http://www.springframework.org/schema/jee" 
	xmlns:tx="http://www.springframework.org/schema/tx"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-4.0.xsd 
	http://www.springframework.org/schema/context  
	http://www.springframework.org/schema/context/spring-context-4.0.xsd
	http://www.springframework.org/schema/jee http://www.springframework.org/schema/jee/spring-jee-4.0.xsd
	http://www.springframework.org/schema/tx 
	http://www.springframework.org/schema/tx/spring-tx.xsd

	">



	<bean id="RoleManager" class="org.mockito.Mockito" factory-method="mock">
		<constructor-arg value="com.agiletec.aps.system.services.role.RoleManager" />
	</bean>
	
	<bean id="GroupManager" class="org.mockito.Mockito" factory-method="mock">
		<constructor-arg value="com.agiletec.aps.system.services.group.GroupManager" />
	</bean>

	<bean id="UserManager" class="org.mockito.Mockito" factory-method="mock">
		<constructor-arg value="com.agiletec.aps.system.services.user.UserManager" />
	</bean>
	
	<bean id="AuthorizationManager" class="org.mockito.Mockito" factory-method="mock">
		<constructor-arg value="com.agiletec.aps.system.services.authorization.AuthorizationManager" />
	</bean>
	
	<bean id="AuthenticationProviderManager"  class="org.mockito.Mockito" factory-method="mock">
		<constructor-arg value="com.agiletec.aps.system.services.user.AuthenticationProviderManager" />
	</bean>

		
	<bean id="OAuth2TokenManager" class="org.mockito.Mockito" factory-method="mock">
		<constructor-arg value="org.entando.entando.aps.system.services.oauth2.ApiOAuth2TokenManager" />
	</bean>

	
</beans>